@{
    Layout = null;
}

<html>
<head>
    <title></title>
    <link href="~/lib/ligerUI/skins/Aqua/css/ligerui-all.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/lib/ligerUI/js/core/base.js"></script>
    <script src="~/lib/ligerUI/js/plugins/ligerDrag.js" type="text/javascript"></script>
    <script src="~/lib/ligerUI/js/plugins/ligerDialog.js" type="text/javascript"></script>
    <script src="~/lib/ligerUI/js/ligerui.all.js"></script>
</head>

<body>
    <div id="mainContainer" class="divFont">
        <div id="fakabianhao" style="clear:both;margin-left:100px; margin-top:50px;  width:600px">
            <label>发卡编号</label>
            <br/>
            <br/>
 
            <label>请输入用户IC卡编号</label>&nbsp;&nbsp;&nbsp;
                    
            <input id="ywbh" type="text" name="ywbh" style="width:200px"/>
            <br/>
            <br/>
            <input type="button" id="submitBtn1" value="确定" style="margin-right:50px;width:50px"/>
            <input type="button" id="submitBtn2" value="取消" style="width:50px"/>
        </div>
    </div>

    <script type="text/javascript">

        $("#submitBtn1").click(function () {
            var ywbh = $("#ywbh").val();
            var postData = {
                ywbh: ywbh
            };
            $.post("../FaKaRecharge/getUserByYwbh", postData, getUserCalllback);
        })
        function getUserCalllback(data) {

            $("#mainContainer").load(data);

        }

    </script>

</body>
</html>

